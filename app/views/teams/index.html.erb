<h4>All Teams</h4>
<h5>Click a team to view!</h5>
<h6>North Texas Teams</h6>
<ul>
  <% @northtxteams.each do |team| %>
    <strong><%= link_to team.name, team_path(team) %></strong></br>
    <span>Sport: <%= team.sport %></span></br>
    <span>City: <%= team.city %></span></br>
    <span>Practice Location: <%= team.practice_location %></span></br>
    <span>Pratice Days: <%= team.practice_days %></span></br>
  <% end %>
</ul>

